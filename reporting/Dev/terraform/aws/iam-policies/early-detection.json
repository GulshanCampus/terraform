{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "s3:AbortMultipartUpload",
        "s3:GetBucketLocation",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:ListBucketMultipartUploads",
        "s3:PutObject"
      ],
      "Resource": [
        "arn:aws:s3:::rpt-codedeploy-dev",
        "arn:aws:s3:::rpt-codedeploy-dev/*",
        "arn:aws:s3:::aws-codedeploy-us-east-1/*"
      ],
      "Effect": "Allow"
    }
  ]
}